{
  "extends": "fastify-tsconfig",
  "compilerOptions": {
    "outDir": "dist",
    "sourceMap": true,
    "moduleResolution": "NodeNext",
    "module": "NodeNext",
    "target": "ES2022",
    "esModuleInterop": true,
    "typeRoots": ["./node_modules/@types", "./path/to/declarations"]
  },
  "include": [
    "src/**/*.ts",
    "uploads/tipos/fastify.ts",
    "src/plugins/multipart.ts"
  ]
}
